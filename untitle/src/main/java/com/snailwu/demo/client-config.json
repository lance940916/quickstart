{
  "log": {
    "access": "/var/v2ray/v2ray-access.log",
    "error": "/var/v2ray/v2ray-error.log",
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 10010,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "settings": {
        "auth": "noauth"
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "tokyo-proxy",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "108.160.139.89",
            "port": 10020,
            "users": [
              {
                "id": "62c75ce7-94b0-46af-a682-4f8517481ad8",
                "alterId": 32,
                "security": "auto"
              }
            ]
          }
        ]
      },
      "mux": {
        "enabled": true
      }
    },
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "blocked",
      "protocol": "blackhole",
      "settings": {}
    }
  ],
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "domain": [
          "geosite:cn"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "ip": [
          "geoip:cn",
          "geoip:private"
        ],
        "outboundTag": "direct"
      }
    ]
  },
  "dns": {
    "servers": [
      {
        "address": "119.29.29.29",
        "port": 53,
        "domains": [
          "geosite:cn"
        ],
        "expectIPs": [
          "geoip:cn"
        ]
      },
      "8.8.8.8",
      "localhost"
    ]
  }
}